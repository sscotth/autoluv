{
  "timeAndCity": {
    "type": "string",
    "pattern": "^[0-9]{1,2}:[0-9]{2} +[PAMpam]{2} +[A-Za-z ()/.]+, +[A-Za-z]{2} +\\([A-Za-z]{3}\\)$"
  },
  "verboseDate": {
    "type": "string",
    "pattern": "^(Sun|Mon|Tues|Wednes|Thurs|Fri|Satur|Sun)day, +(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec) +[0-9]{1,2}, +[0-9]{4}$"
  }
}
