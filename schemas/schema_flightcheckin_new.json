{
  "type": "object",
  "required": ["errmsg", "passenger_names", "output"],
  "properties": {
    "errmsg": { "type": "string" },
    "passenger_names" : {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["passengerName"],
        "properties": {
          "passengerName": { "type": "string" }
        }
      }
    },
    "output": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "require": ["departCityCode", "travelDate", "departTime", "flightNumber"],
        "properties": {
          "departCityCode" : { "type": "string" },
          "travelDate" : { "type": "string" },
          "departTime" : { "type": "string" },
          "flightNumber" : { "type": "string" }
        }
      }
    }
  }
}